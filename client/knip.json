{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/App.tsx"],
  "ignore": [
    "node_modules/**",
    "dist/**",
    "build/**",
    "public/**",
    "src/assets/**",
    "src/**/*.test.*",
    "src/**/__tests__/**",
    "src/**/*.spec.*",
    "src/**/*.stories.*"
  ],
  "ignoreBinaries": ["nginx"],
  "project": ["**/*.{ts,tsx,js,jsx,html,css}"],
  "tags": ["lintignore"]
}
